{{controller.length}} Items from {{products.length}} Products

<table class="table table-condensed">
  <thead>
    <tr>
      <th>Title</th>
      <th>Product</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each controller}}
      <tr>
        <td><span class="badge">{{score}}</span> {{title}}</td>
        <td>{{product.name}}</td>
        <td>{{status}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
